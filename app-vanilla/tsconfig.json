{
	"compilerOptions": {
		"outDir": "../remote_modules/app",
		"sourceMap": true,
		"noEmit": false,
		"target": "ESNext",
		"lib": ["ESNext", "DOM"],
		"module": "ESNext",
		"moduleResolution": "Node",
		"strict": false,
		"allowJs": true,
		"allowSyntheticDefaultImports": true,
		"checkJs": true,
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"skipLibCheck": true
	},
	"include": ["src", "../node_modules/webpack/module.d.ts", "../node_modules/webpack-dev-server/types/lib/Server.js"]
}
